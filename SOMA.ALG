Algoritmo "soma"

Var
  resultado, n1, n2 :  inteiro

Inicio

  escreva ("Entre com o primeiro número: ")
  leia (n1)

  escreva ("Entre com o segundo número: ")
  leia (n2)
  
  // Realizando a soma dos dois números:
  resultado <-n1 + n2
  
  escreva ("A soma dos números é: ",resultado)

Fimalgoritmo
